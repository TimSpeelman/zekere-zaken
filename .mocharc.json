{
    "diff": true,
    "extension": [
        "ts"
    ],
    "package": "./package.json",
    "reporter": "spec",
    "slow": 75,
    "timeout": 30000,
    "require": "ts-node/register",
    "watch-files": [
        "src/**/*.ts",
        "test/**/*.ts"
    ],
    "watch-ignore": [
        "node_modules/"
    ]
}